{
  "version": 2,
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/Login.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Login.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}